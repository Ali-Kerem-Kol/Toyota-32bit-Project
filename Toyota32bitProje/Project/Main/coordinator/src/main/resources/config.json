{
  "providers": [
    {
      "className": "com.mydomain.main.provider.TCPProvider",
      "platformName": "TCP_PLATFORM",
      "subscribeRates": ["PF1_USDTRY","PF1_EURUSD","PF1_GBPUSD"]
    },
    {
      "className": "com.mydomain.main.provider.RESTProvider",
      "platformName": "REST_PLATFORM",
      "subscribeRates": ["PF2_USDTRY","PF2_EURUSD","PF2_GBPUSD"]
    }
  ],
  "calculation": {
    "calculationMethod": "javascript",
    "formulaFilePath": "/app/scripts/Formula.js"
  },
  "kafka": {
    "bootstrapServers": "kafka:9092",
    "topicName": "rates-topic",
    "acks": "all",
    "retries": 3,
    "deliveryTimeoutMs": 30000,
    "requestTimeoutMs": 15000,
    "reinitPeriodSec": 5,
    "lingerMs": 0
  },
  "redis": {
    "host": "redis",
    "port": 6379,
    "database": 0,
    "password": "",
    "rawTtl": 10,
    "calculatedTtl": 10,
    "rawStream": "raw_rates",
    "calculatedStream": "calculated_rates",
    "streamMaxLen": 10000,
    "rawGroup": "raw_group",
    "calcGroup": "calc_group",
    "rawConsumer": "raw_consumer",
    "calcConsumer": "calc_consumer",
    "streamReadCount": 10,
    "streamBlockMillis": 5000
  }
}